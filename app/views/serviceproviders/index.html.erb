<p id="notice"><%= notice %></p>

<h1>Serviceproviders</h1>

<table>
  <thead>
    <tr>
      <th>Providername</th>
      <th>Providerservice</th>
      <th>Mailid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @serviceproviders.each do |serviceprovider| %>
      <tr>
        <td><%= serviceprovider.providername %></td>
        <td><%= serviceprovider.providerservice %></td>
        <td><%= serviceprovider.mailid %></td>
        <td><%= link_to 'Show', serviceprovider %></td>
        <td><%= link_to 'Edit', edit_serviceprovider_path(serviceprovider) %></td>
        <td><%= link_to 'Destroy', serviceprovider, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Serviceprovider', new_serviceprovider_path %>
